      "git add"
    ],
    "*.{ts}": "npm run test:lint:ts",
    "*.md": "npm run test:lint:md"
  },
  "name": "@joncursi/nest-base",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joncursi/nest-base.git"
