  "scripts": {
    "test": "npm-run-all test:**",
    "test:lint:js": "eslint . --ext .js --quiet",
    "test:lint:md": "markdownlint README.md --config node_modules/@joncursi/eslint-config/markdownlint.config.json",
    "test:lint:ts": "eslint . --ext .ts --quiet"
  },
  "version": "1.0.0"
